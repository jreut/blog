{
  "scripts": {
    "clean": "rm -rf dist && mkdir -p dist/entries dist/stylesheets",
    "lint": "eslint ./",
    "lint:fix": "eslint ./ --fix",
    "meta": "cp src/humans.txt dist/humans.txt",
    "assets": "cp -r src/graphics dist/graphics",
    "predist": "yarn run clean && yarn run lint",
    "dist": "yarn run meta && yarn run assets && bin/dist",
    "preserve": "npm run dist",
    "serve": "node index.js",
    "watch": "bin/watch",
    "predeploy:pre": "yarn run dist",
    "predeploy:production": "yarn run dist",
    "deploy:pre": "stout deploy -env development",
    "deploy:production": "stout deploy -env production"
  },
  "devDependencies": {
    "cheerio": "^1.0.0-rc.1",
    "chokidar": "^1.7.0",
    "connect": "^3.6.2",
    "cssnano": "^3.10.0",
    "eslint": "^4.0.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-node": "^5.0.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "folktale": "^2.0.0-beta1",
    "inject-lr-script": "^2.1.0",
    "moment": "^2.18.1",
    "moment-timezone": "^0.5.13",
    "morgan": "^1.8.2",
    "mustache": "^2.3.0",
    "pointfree-fantasy": "^0.1.3",
    "postcss": "^6.0.2",
    "postcss-cssnext": "^2.11.0",
    "serve-static": "^1.12.3",
    "tiny-lr": "^1.0.5"
  },
  "dependencies": {}
}
